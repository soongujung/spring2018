<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.scrapper.web.user">
	<select id="selectUserForLogin" parameterType="java.util.HashMap" resultType="com.scrapper.web.user.vo.UserVO">
		SELECT 	* 
		FROM	User 
		WHERE 	userId=#{userId} AND password=#{password}
	</select>
</mapper>